{#
/**
 * @file
 * Default theme implementation for the Bynder search result item.
 *
 * Available variables:
 * - thumbnail_uri: Link to the thumbnail of the bynder asset.
 * - name: Name of the bynder asset.
 * - type: Type of the bydner asset.
 */
#}
	{% for image, key in images %}
        <img src="{{key.thumb}}" alt="{{key.id}}"/>
        <input data-drupal-selector="edit-thumbnails-{{ key.id }}" type="checkbox" id="edit-thumbnails-{{ key.id }}" name="thumbnails[{{ key.id }}]" value="{{ key.id }}" class="form-checkbox">
    {% endfor %}
